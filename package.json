    "@types/obsidian-typings": "npm:obsidian-typings@^1.1.6",
    "ansi-colors": "^4.1.3",
    "builtin-modules": "4.0.0",
    "commander": "^12.1.0",
    "commit-and-tag-version": "^12.4.1",
    "cross-env": "^7.0.3",
    "dedent": "^1.5.3",
    "dotenv": "^16.4.5",
    "esbuild": "0.23.0",
    "eslint": "^9.6.0",
    "klona": "^2.0.6",
