{
  "name": "create-note-in-folder",
  "version": "1.12.3",
  "description": "Add commands to create a note in a specific folder.",
  "main": "main.js",
  "private": true,
  "scripts": {
    "prebuild": "tsc --noEmit --skipLibCheck",
    "build": "node esbuild.config.mjs --production",
    "dev:prod": "node esbuild.config.mjs --vault",
